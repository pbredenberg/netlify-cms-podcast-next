{
  "name": "netlify-cms-podcast-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "build:cms": "webpack --config weback.config.cms.js",
    "build:feed": "ts-node --transpile-only scripts/generate-podcast-feed.ts",
    "cms": "npm run build && concurrently \"npm run build:cms -- --watch\" \"netlify-cms-proxy-server\" \"http-server -c-1 ./out\"",
    "start": "next start",
    "export": "npm run build && next export"
  },
  "dependencies": {
    "netlify-cms-app": "^2.14.46",
    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-react": "^7.13.13",
    "@types/node": "^14.14.41",
    "@types/react": "17.0.3",
    "concurrently": "^6.0.2",
    "css-loader": "^5.2.4",
    "file-loader": "^6.2.0",
    "frontmatter-markdown-loader": "^3.6.2",
    "gray-matter": "^4.0.2",
    "http-server": "0.12.3",
    "mini-css-extract-plugin": "^1.5.0",
    "netlify-cms-proxy-server": "^1.3.12",
    "sass": "1.32.11",
    "sass-loader": "^11.0.1",
    "ts-loader": "^9.1.0",
    "ts-node": "^10.0.0",
    "typescript": "4.2.4",
    "webpack": "^5.35.0",
    "webpack-cli": "^4.6.0"
  }
}
